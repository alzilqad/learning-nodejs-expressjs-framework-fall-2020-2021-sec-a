<body>
  <div class="wrap">
    <header id="header">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <button id="primary-nav-button" type="button">Menu</button>
            <a href="index.html"
              ><div class="logo">
                <img src="/img/logo.png" alt="Venue Logo" /></div
            ></a>
            <nav id="primary-nav" class="dropdown cf">
              <ul class="dropdown menu">
                <li>
                  <a href="/">Home</a>
                </li>

                <!-- admin header -->
                <% if(type!="") { %> <% if(type==="admin") { %>
                <li>
                  <a class="scrollTo" href="/scout">Scouts</a>
                </li>
                <li>
                  <a class="scrollTo" href="/user">Users</a>
                </li>
                <li>
                  <a class="scrollTo" href="/place">Destination</a>
                </li>

                <!-- scout header -->
                <% } else if(type==="scout") { %>
                <li>
                  <a class="scrollTo" href="/place">Destination</a>
                </li>
                <% } %>

                <!-- register user header -->
                <li>
                  <a class="scrollTo" data-scrollTo="wishlist" href="/wishlist"
                    >Wishlist</a
                  >
                </li>
                <li>
                  <a class="scrollTo" href="/logout">Logout</a>
                </li>

                <!-- non-register user header -->
                <% } else { %>
                <li>
                  <a class="scrollTo" data-scrollTo="contact" href="#"
                    >Contact Us</a
                  >
                </li>
                <li>
                  <a class="scrollTo" href="/login">Login</a>
                </li>
                <% } %>

                <li>
                  <a
                    class="scrollTo"
                    data-scrollTo="registration"
                    href="/registration"
                    >Registration</a
                  >
                </li>
              </ul>
            </nav>
            <!-- / #primary-nav -->
          </div>
        </div>
      </div>
    </header>
  </div>
